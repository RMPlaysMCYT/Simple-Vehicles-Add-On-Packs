{
    "namespace": "debug_screen",
    "access_button@common_buttons.light_content_button": {
        "$pressed_button_name": "button.access",
        "size": [20, 20],
        "offset": [20, 20],
        "anchor_to": "top_left",
        "anchor_from": "top_left",
        "bindings": [{
                "binding_name": "#access_screen_visible",
                "binding_name_override": "#visible"
            }
        ]
    },
    "rmplaysmcyt_watermark": {
        "type": "label",
        "text": "Simple Vehicles Addon Version 1.7.0\nMade by RMPlaysMC YT in §rCollaboration with:\n§bChrisJoyCraft55 §rand §4ANDONCRAFT\n§rMade Exclusively for MCPEDL and MCPECORE\n§aMade for Minecraft 1.16.100 and up..",
        "color": [1.0, 1.0, 1.0],
        "shadow": true,
        "font_size": "small",
        "anchor_from": "top_right",
        "anchor_to": "top_right",
        "alpha": 10
    },
    "rmplaysmcyt_watermark_image": {
        "type": "image",
        "texture": "textures/ui/watermark",
        "layer": 3,
        "offset": [-112, 2],
        "anchor_from": "top_right",
        "anchor_to": "top_right",
        "size": [24, 24]
    },
    "special_render": {
        "type": "custom",
        "renderer": "debug_screen_renderer"
    },
    "content_panel": {
        "type": "panel",
        "clips_children": false,
        "size": ["100%", "100%"],
        "controls": [{
                "access_button@debug_screen.access_button": {}
            }, {
                "special_render@debug_screen.special_render": {}                           
            }, {
                "rmplaysmcyt_watermark@debug_screen.rmplaysmcyt_watermark": {}
            }, {
                "rmplaysmcyt_watermark_image@debug_screen.rmplaysmcyt_watermark_image": {}
            }
        ]
    },
    "debug_screen@common.base_screen": {
        "send_telemetry": false,
        "screen_not_flushable": true,
        "render_game_behind": true,
        "is_showing_menu": false,
        "low_frequency_rendering": true,
        "is_modal": true,
        "should_steal_mouse": false,
        "render_only_when_topmost": false,
        "screen_draws_last": true,
        "always_accepts_input": true,
        "$screen_content": "debug_screen.content_panel",
        "$screen_animations": [],
        "$use_loading_bars": false
    }
}