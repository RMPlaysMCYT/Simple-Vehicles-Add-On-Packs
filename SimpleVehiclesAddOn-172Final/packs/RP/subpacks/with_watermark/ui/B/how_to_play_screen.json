{
"namespace": "how_to_play",

  "how_to_play_screen@how_to_play_common.screen_base": {
    "$selector_stack_panel": "how_to_play.selector_stack_panel",
    "$section_content_panels": "how_to_play.section_content_panels",
    "$header_safezone_control": "common_store.store_top_bar_filler",
    "$header_bar_control": "common_store.store_top_bar",    
    "$is_full_screen_layout": true
  },

  "selector_stack_panel": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "orientation": "vertical",
    "$default_selector_toggle_index": 2,
    "controls": [
      {
        "how_to_play_selector_pane": {
          "type": "stack_panel",
          "controls": [
            {
              "padding_0": {
                "type": "panel",
                "size": [ 0, 6 ]
              }
            },
            { 
              "simveh_information_category@how_to_play_common.topic_category": { 
                "$category": "info" 
              }
            },
            { 
              "simveh_info_button@how_to_play.simveh_info_button": { 
                "$toggle_group_forced_index": 0 
              } 
            },
            { 
              "simveh_welcome_button@how_to_play.simveh_welcome_button": { 
                "$toggle_group_forced_index": 1 
              } 
            },
            { 
              "simveh_credits_button@how_to_play.simveh_credits_button": { 
                "$toggle_group_forced_index": 2 
              } 
            },
            { 
              "simveh_vehicles_category@how_to_play_common.topic_category": { 
                "$category": "vehicles" 
              }
            },
            { 
              "simveh_cars_button@how_to_play.simveh_cars_button": { 
                "$toggle_group_forced_index": 3 
              } 
            },
            { 
              "simveh_public_commuter_button@how_to_play.simveh_public_commuter_button": { 
                "$toggle_group_forced_index": 4 
              } 
            },
            { 
              "simveh_public_service_button@how_to_play.simveh_public_service_button": { 
                "$toggle_group_forced_index": 5 
              } 
            },
            { 
              "simveh_flying_button@how_to_play.simveh_flying_button": { 
                "$toggle_group_forced_index": 6 
              } 
            },
            { 
              "simveh_sea_button@how_to_play.simveh_sea_button": { 
                "$toggle_group_forced_index": 7 
              } 
            },
            { 
              "simveh_aquatic_button@how_to_play.simveh_aquatic_button": { 
                "$toggle_group_forced_index": 8 
              } 
            },
            { 
              "simveh_luxury_button@how_to_play.simveh_luxury_button": { 
                "$toggle_group_forced_index": 9 
              } 
            }
          ]
        }
      }
    ]
  },

  "section_content_panels": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ "100% - 5px", "100%c + 5px" ],
    "offset": [ 2, 0 ],
    "controls": [
      {
        "general_tips_sections": {
          "type": "stack_panel",
          "controls": [
            { 
              "section_contents_header@how_to_play_common.section_contents_header": { }
            },
            { 
              "simveh_info_section@how_to_play.simveh_info_section": { } 
            },
            { 
              "simveh_welcome_section@how_to_play.simveh_welcome_section": { } 
            },
            { 
              "simveh_credits_section@how_to_play.simveh_credits_section": { } 
            },
            { 
              "simveh_cars_section@how_to_play.simveh_cars_section": { } 
            },
            { 
              "simveh_public_commuter_section@how_to_play.simveh_public_commuter_section": { } 
            },
            { 
              "simveh_public_service_section@how_to_play.simveh_public_service_section": { } 
            },
            { 
              "simveh_flying_section@how_to_play.simveh_flying_section": { } 
            },
            { 
              "simveh_sea_section@how_to_play.simveh_sea_section": { } 
            },
            { 
              "simveh_aquatic_section@how_to_play.simveh_aquatic_section": { } 
            },
            { 
              "simveh_luxury_section@how_to_play.simveh_luxury_section": { } 
            },
            {
              "section_contents_footer@how_to_play_common.section_contents_footer": {
              }
            }            
          ]
        }
      }
    ]
  },
