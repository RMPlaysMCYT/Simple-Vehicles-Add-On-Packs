{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.scooter_ride": {
			"states": {
				"default": {
					"transitions": [
						{
							"riding": "query.has_rider"
						}
					],
					"on_entry": [
						"/function player_ride_scooter"
					]
				},
				"riding": {
					"transitions": [
						{
							"default": "!query.has_rider"
						}
					],
					"on_entry": [
						"/function player_ride_scooter",
						"/function honk"
					]
				}
			}
		}
	}
}