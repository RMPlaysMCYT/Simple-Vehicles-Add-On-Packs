{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.armored_truck.item": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"riding": "query.has_rider"
						}
					]
				},
				"riding": {
					"transitions": [
						{
							"default": "!query.has_rider"
						}
					],
					"on_entry": [
						"/function item_armored_truck"
					]
				}
			}
		}
	}
}