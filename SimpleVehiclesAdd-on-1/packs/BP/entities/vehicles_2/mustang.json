{
  "format_version": "1.13.0",
  "minecraft:entity": {
    "description": {
      "identifier": "aspire:ford_mustang",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "minecraft:ranger": {
        "minecraft:is_tamed": {},
        "minecraft:rideable": {
          "seat_count": 2,
          "crouching_skip_interact": true,
          "family_types": ["player"],
          "interact_text": "Drive",
          "seats": [
            {
              "position": [0.5, 0.1, 0]
            },
            {
              "position": [-0.5, 0.1, 0]
            }
          ]
        }
      },
      "minecraft:bugfixer": {
        "minecraft:timer": {
          "time": [0.1, 0.1],
          "looping": false,
          "time_down_event": {
            "event": "minecraft:bugfix",
            "target": "self"
          }
        }
      },
      "minecraft:horse_saddled": {
        "minecraft:input_ground_controlled": {},
        "minecraft:can_power_jump": {},
        "minecraft:behavior.player_ride_tamed": {}
      },
      "minecraft:red": {
        "minecraft:variant": {
          "value": 0
        }
      },
      "minecraft:blue": {
        "minecraft:variant": {
          "value": 1
        }
      }
    },
    "components": {
      "minecraft:scale": {
        "value": 1.05
      },
      "minecraft:equippable": {
        "slots": [
          {
            "slot": 0,
            "item": "aspire:key",
            "accepted_items": ["aspire:key"],
            "on_equip": {
              "event": "minecraft:horse_saddled"
            },
            "on_unequip": {
              "event": "minecraft:horse_unsaddled"
            }
          }
        ]
      },
      "minecraft:type_family": {
        "family": ["horse"]
      },
      "minecraft:collision_box": {
        "width": 1.4,
        "height": 1.6
      },
      "minecraft:health": {
        "value": {
          "range_min": 15,
          "range_max": 15
        }
      },
      "minecraft:inventory": {
        "inventory_size": 15,
        "container_type": "horse",
        "private": false
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "fall",
            "deals_damage": "no"
          }
        ]
      },
      "minecraft:movement": {
        "value": {
          "range_min": 0.1125,
          "range_max": 4.3375
        }
      },
      "minecraft:loot": {
        "table": "loot_tables/entities/mustang.json"
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "avoid_water": false,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:nameable": {},
      "minecraft:horse.jump_strength": {
        "value": {
          "range_min": 0.4,
          "range_max": 1.0
        }
      },
      "minecraft:balloonable": {},
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:is_chested": {},
      "minecraft:interact": {
        "interactions": [
          {
            "on_interact": {
              "filters": {
                "all_of": [
                  {
                    "test": "has_equipment",
                    "subject": "other",
                    "domain": "hand",
                    "value": "dye:1"
                  },
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "player"
                  },
                  {
                    "test": "has_ability",
                    "subject": "other",
                    "value": "instabuild"
                  }
                ]
              },
              "event": "minecraft:turn_red"
            },
            "use_item": true,
            "play_sounds": "explode",
            "interact_text": "Paint into Red?"
          },
          {
            "on_interact": {
              "filters": {
                "all_of": [
                  {
                    "test": "has_equipment",
                    "subject": "other",
                    "domain": "hand",
                    "value": "dye:18"
                  },
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "player"
                  },
                  {
                    "test": "has_ability",
                    "subject": "other",
                    "value": "instabuild"
                  }
                ]
              },
              "event": "minecraft:turn_blue"
            },
            "use_item": true,
            "play_sounds": "explode",
            "interact_text": "Paint into Blue?"
          }
        ]
      },
      "minecraft:physics": {}
    },
    "events": {
      "minecraft:entity_spawned": {
        "sequence": [
          {
            "randomize": [
              {
                "weight": 360,
                "add": {
                  "component_groups": ["minecraft:ranger"]
                }
              }
            ]
          },
          {
            "randomize": [
              {
                "weight": 100,
                "add": {
                  "component_groups": ["minecraft:red"]
                }
              },
              {
                "weight": 100,
                "add": {
                  "component_groups": ["minecraft:blue"]
                }
              }
            ]
          }
        ]
      },
      "minecraft:turn_red": {
        "add": {
          "component_groups": ["minecraft:red"]
        }
      },
      "minecraft:turn_blue": {
        "add": {
          "component_groups": ["minecraft:blue"]
        }
      },
      "minecraft:horse_saddled": {
        "add": {
          "component_groups": ["minecraft:horse_saddled"]
        }
      },
      "minecraft:horse_unsaddled": {
        "add": {
          "component_groups": ["minecraft:bugfixer"]
        },
        "remove": {
          "component_groups": ["minecraft:horse_saddled"]
        }
      },
      "minecraft:bugfix": {
        "remove": {
          "component_groups": ["minecraft:bugfixer"]
        }
      }
    }
  }
}
