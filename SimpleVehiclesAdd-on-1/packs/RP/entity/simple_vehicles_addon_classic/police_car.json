{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "aspire:police_car",
      "materials": {
        "default": "witch"
      },
      "textures": {
        "default": "textures/simple_vehicles_classic/entity/police_car"
      },
      "geometry": {
        "default": "geometry.simple_vehicles_classic_police_car"
      },
      "spawn_egg": {
        "texture": "aspire_vehicles",
        "texture_index": 13
      },
      "animations": {
        "move": "animation.police_car.move"
      },
      "scripts": {
        "initialize": ["variable.steering = 0;"],
        "pre_animation": [
          "query.has_rider ? (variable.steering = Math.lerp(variable.steering, query.yaw_speed, 0.15));"
        ],
        "animate": ["move"]
      },
      "render_controllers": ["controller.render.bus"]
    }
  }
}
