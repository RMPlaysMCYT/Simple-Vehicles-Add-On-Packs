{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "aspire:car",
      "materials": {
        "default": "aspire"
      },
      "textures": {
        "grey": "textures/entity/car",
        "red": "textures/entity/car/car_red",
        "green": "textures/entity/car/car_green",
        "blue": "textures/entity/car/car_blue",
        "yellow": "textures/entity/car/car_yellow",
        "pink": "textures/entity/car/car_pink",
        "black": "textures/entity/car/car_black",
        "purple": "textures/entity/car/car_purple",
        "orange": "textures/entity/car/car_naranja",
        "brown": "textures/entity/car/car_cafe",
        "cyan": "textures/entity/car/car_cyan"
      },
      "geometry": {
        "default": "geometry.car"
      },
      "animations": {
        "move": "animation.car.move"
      },
      "scripts": {
        "initialize": ["variable.steering = 0;"],
        "pre_animation": [
          "query.has_rider ? (variable.steering = Math.lerp(variable.steering, query.yaw_speed, 0.15));"
        ],
        "animate": ["move"]
      },
      "render_controllers": ["controller.render.car"],
      "spawn_egg": {
        "texture": "Vehicles",
        "texture_index": 4
      }
    }
  }
}
